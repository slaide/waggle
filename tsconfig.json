{
    "compilerOptions": {
        "module": "System",
        "target": "ES2023",
        "checkJs": true,
        "strict": true,
        "allowJs": true,
        "noImplicitAny": true,
        "alwaysStrict": true,
        "lib": [
            "ES5",
            "ES2015","ES2016","ES2017","ES2018","ES2019",
            "ES2020","ES2021","ES2022","ES2023","ES2024",
            "DOM"
        ],
        "noEmit": true,
        "rootDir": ".",
        "typeRoots": ["types"],
        "paths":{
            "png":["./static/src/png.js"]
        }
    },
    "include": [
        "static/src/**/*.js",
        "types/*.d.ts",
        "types/**/*.d.ts"
    ]
}
  