{
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "node10",
        "target": "ES2023",
        "checkJs": true,
        "strict": true,
        "allowJs": true,
        "noImplicitAny": true,
        "alwaysStrict": true,
        "lib": [
            "ES2023","DOM"
        ],
        "rootDir": ".",
        "outDir": ".",
        "typeRoots": ["types"],
        "paths":{ }
    },
    "exclude": [
        "static/src/**/*.as.ts",
        "static/dep/**/*"
    ],
    "include": [
        "static/src/**/*.ts"
    ]
}
  